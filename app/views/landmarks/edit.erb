<form action="/landmarks/<%=@landmark.id%>" method="post">
<input name="_method" type="hidden" value="patch">
  <h1><%=@landmark.name%></h1>

  <label>Landmark Name </label>
  <input type="text" name="landmark[name]" id="name" value="<%=@landmark.name%>"><br>
   
  <label>Year Completed </label>
  <input type="text" name="landmark[year_completed]" id="year_completed" value="<%=@landmark.year_completed%>"><br><br>

  <input type="submit" value="Edit Landmark">
</form>    